# Environment variables for Firebase Hosting deployment
# Copy this file to .env.production.local and update with your production values

# Deepgram API Key (get from https://console.deepgram.com/)
NEXT_PUBLIC_DEEPGRAM_API_KEY=your_production_deepgram_api_key_here

# Verbum API Key (get from your Verbum dashboard)
NEXT_PUBLIC_VERBUM_API_KEY=your_production_verbum_api_key_here

# Optional: Analytics or other tracking IDs
# NEXT_PUBLIC_GA_ID=your_google_analytics_id
# NEXT_PUBLIC_FIREBASE_CONFIG=your_firebase_config_json