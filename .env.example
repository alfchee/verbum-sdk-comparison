# Copy this file to .env.local and add your real API keys

# Verbum API Key - Get from https://sdk.verbum.ai/
NEXT_PUBLIC_VERBUM_API_KEY=your_verbum_api_key_here

# Deepgram API Key - Get from https://deepgram.com/
NEXT_PUBLIC_DEEPGRAM_API_KEY=your_deepgram_api_key_here

# Optional: Application URL for CORS
NEXT_PUBLIC_APP_URL=http://localhost:3000